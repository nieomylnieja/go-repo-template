{
  "allowCompoundWords": true,
  "language": "en_US",
  "dictionaries": [
    "bash",
    "css",
    "golang",
    "html",
    "misc",
    "softwareTerms",
    "npm",
    "typescript"
  ],
  "ignorePaths": [
    ".vscode/",
    ".idea/",
    "**/node_modules/**",
    "**/*.lock",
    "package.json"
  ],
  "ignoreRegExpList": [
    "/\\|\\s+[T|G]{2,}\\s+\\|/g",
    "/https?:\\/\\/.*/g",
    "/github.*/g",
    "/nolint.*/g",
    "/nosec.*/g"
  ],
  "words": []
}